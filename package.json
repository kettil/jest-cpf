{
  "name": "jest-cpf",
  "version": "0.1.0",
  "description": "Generates a coverage per file overview",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "bin": "build/bin/jest-cpf.js",
  "scripts": {
    "build": "tsc --project ./tsconfig.build.json",
    "prebuild": "rm -rf ./build",
    "test": "jest --config=jest.json --coverage",
    "test:watch": "jest --config=jest.json --coverage --watch 'src'",
    "test:ci": "jest --config=jest.ci.js --ci --coverage --verbose",
    "pretest": "rm -rf ./coverage",
    "coverage": "node ./build/bin/jest-cpf.js --config=jest.json",
    "precoverage": "npm run build",
    "git:change": "npm install",
    "git:commit": "npm run prettier:ci && npm run lint:ci",
    "git:push": "npm run git:commit && npm run test:ci",
    "lint": "eslint --config .eslintrc.json --ext=ts,tsx --cache --fix .",
    "lint:ci": "eslint --config .eslintrc.json --ext=ts,tsx --format codeframe .",
    "prelint": "npm run prettier",
    "prettier": "prettier --config './.prettierrc' --write '.huskyrc' '.prettierrc' './**/*.{ts,tsx,js,jsx,json,md,scss,yml,html}'",
    "prettier:ci": "prettier --config './.prettierrc' --list-different '.huskyrc' '.prettierrc' './**/*.{ts,tsx,js,jsx,json,md,scss,yml,html}'",
    "deploy:major": "npm run deploy:pre && npm version major -m 'Release of version v%s' && npm run deploy:post",
    "deploy:minor": "npm run deploy:pre && npm version minor -m 'Release of version v%s' && npm run deploy:post",
    "deploy:patch": "npm run deploy:pre && npm version patch -m 'Release of version v%s' && npm run deploy:post",
    "deploy:pre": "npm run lint && npm run test && npm run build",
    "deploy:post": "git push && git push --tags && npm publish"
  },
  "keywords": [
    "jest",
    "coverage",
    "unit",
    "unit test",
    "integration",
    "integration test",
    "test",
    "testing"
  ],
  "author": "Kjell Die√üel <development@kettil.de>",
  "license": "MIT",
  "devDependencies": {
    "@types/cli-progress": "^3.4.2",
    "@types/glob": "^7.1.1",
    "@types/jest": "^25.1.2",
    "@types/luxon": "^1.21.0",
    "@types/node": "^13.7.1",
    "@types/table": "^4.0.7",
    "@types/yargs": "^15.0.3",
    "@typescript-eslint/eslint-plugin": "^2.19.2",
    "@typescript-eslint/parser": "^2.19.2",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.7.0",
    "eslint-plugin-unicorn": "^16.1.1",
    "husky": "^4.2.3",
    "jest": "^25.0.0",
    "prettier": "^1.0.0",
    "ts-jest": "^25.2.0",
    "typescript": "^3.4.0"
  },
  "dependencies": {
    "chalk": "^3.0.0",
    "cli-progress": "^3.6.0",
    "glob": "^7.1.6",
    "luxon": "^1.22.0",
    "table": "^5.4.6",
    "yargs": "^15.1.0"
  }
}
